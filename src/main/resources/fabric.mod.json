{
  "schemaVersion": 1,
  "id": "worldshell_fork",
  "version": "${version}",
  "name": "WorldShell Fork",
  "description": "A library mod to allow moving collections of blocks that can function, collide and interact with the world around them.",
  "authors": [
    "Snakefangox", "Sorenon"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/worldshell/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "net.snakefangox.worldshell.client.WSClient"
    ],
    "main": [
      "net.snakefangox.worldshell.WorldShellMain"
    ]
  },
  "mixins": [
    "worldshell_fork.mixins.json"
  ],
  "accessWidener": "worldshell.accesswidener",
  "depends": {
    "fabricloader": ">=0.11.0",
    "fabric": "*",
    "minecraft": ">=1.17"
  },
  "custom": {
    "modmenu": {
      "badges": [ "library" ],
      "parent": "grappleship"
    }
  }
}
